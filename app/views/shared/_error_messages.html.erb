<!--партиал для отображения сообщений об ошибках отправки формы регистрации-->
<% if object.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-error">

      Всего ошибок: <%= pluralize(object.errors.count, "error") %>.
    </div>
    <ul>
    <% object.errors.full_messages.each do |msg| %>
      <li>* <%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
